{
  "rewrites": [
    {
      "source": "/((?!api/.*).*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/waitlist.js": {
      "maxDuration": 30
    },
    "api/test-database.js": {
      "maxDuration": 30
    },
    "api/check-env.js": {
      "maxDuration": 10
    },
    "api/users/identify.js": {
      "maxDuration": 30
    },
    "api/phonepe/*.py": {
      "maxDuration": 30
    },
    "api/webhooks/*.py": {
      "maxDuration": 30
    },
    "api/health.py": {
      "maxDuration": 10
    }
  }
}